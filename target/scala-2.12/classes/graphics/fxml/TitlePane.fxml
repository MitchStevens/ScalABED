<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane id="pane-root" fx:id="title_pane" prefHeight="768.0" prefWidth="1024.0"
           xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="graphics.TitlePane">
    <VBox>
        <Label styleClass="title" text="Digital Sage"/>
        <Label onMouseClicked="#play_clicked" styleClass="a" text="Play"/>
        <Label onMouseClicked="#sandbox_clicked" styleClass="a" text="Sandbox"/>
        <Label onMouseClicked="#exit_clicked" styleClass="a" text="Exit"/>
    </VBox>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <stylesheets>
        <URL value="@../css/all-panes.css"/>
        <URL value="@../css/title-pane.css"/>
    </stylesheets>
</StackPane>
